        "babel-jest": "^26.6.3",
        "eslint": "^7.14.0",
        "jest": "^26.6.3",
        "jest-junit": "^12.0.0",
        "mongoose": "^5.10.19",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.4.4",
